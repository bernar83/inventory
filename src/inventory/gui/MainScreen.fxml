        <Label layoutX="14.0" layoutY="90.0" text="Parts">
            <font>
                <Font name="System Bold" size="15.0"/>
            </font>
        </Label>
        
        <TableView layoutX="14.0" layoutY="115.0" prefHeight="175.0" prefWidth="500.0">
            <columns>
                <TableColumn prefWidth="100.0" text="Part ID"/>
                <TableColumn prefWidth="100.0" text="Part Name"/>
                <TableColumn prefWidth="125.0" text="Inventory Level"/>
                <TableColumn prefWidth="175.0" text="Price/Cost per Unit"/>
            </columns>
        </TableView>
        
        <Label layoutX="525.0" layoutY="90.0" text="Products">
            <font>
                <Font name="System Bold" size="15.0"/>
            </font>
        </Label>
        
        <TableView layoutX="525.0" layoutY="115.0" prefHeight="175.0" prefWidth="500.0">
            <columns>
                <TableColumn prefWidth="100.0" text="Product ID"/>
                <TableColumn prefWidth="100.0" text="Product Name"/>
                <TableColumn prefWidth="125.0" text="Inventory Level"/>
                <TableColumn prefWidth="175.0" text="Price per Unit"/>
            </columns>
